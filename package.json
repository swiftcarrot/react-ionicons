{
  "name": "@xkit/ionicons",
  "version": "0.1.21",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "description": "ionicons react component",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "NODE_ENV=production yarn clean && yarn build-icons && yarn build-index && rollup -c",
    "test": "npm run build",
    "build-icons": "mkdir -p src && svgr --filename-case kebab -d src `node -e \"console.log(path.resolve(require.resolve('ionicons'), '../ionicons/svg'))\"`",
    "build-index": "node build.js > src/index.js",
    "clean": "rm -rf src && rm -rf dist",
    "compile": "babel icons --out-dir dist",
    "prepublishOnly": "yarn build"
  },
  "author": "Wang Zuo <wzuoadjusted@gmail.com> (https://wangzuo.me)",
  "license": "MIT",
  "devDependencies": {
    "@svgr/cli": "^4.1.0",
    "babel-preset-zkit": "^0.3.0",
    "fast-glob": "^2.2.6",
    "ionicons": "^4.5.5",
    "lodash": "^4.17.11",
    "rollup": "^1.6.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangzuo/ionicons.git"
  },
  "keywords": [
    "react-component",
    "icon",
    "svg",
    "ionicons"
  ],
  "bugs": {
    "url": "https://github.com/wangzuo/ionicons/issues"
  },
  "homepage": "https://github.com/wangzuo/ionicons#readme"
}
